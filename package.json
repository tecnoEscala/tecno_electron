{
  "name": "tecnoescala",
  "product_name": "Tecnoescala",
  "version": "1.8.11",
  "description": "Aplicacion tecnoescala",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "watch": "nodemon --exec electron .",
    "build-win": "electron-builder --win"
  },
  "build": {
    "appId": "tecnoescala",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.ico"
    },
    "nsis": {
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "uninstallDisplayName": "Tecnoescala",
      "license": "LICENSE.md"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^14.0.0",
    "electron-builder": "^22.13.1",
    "nodemon": "^3.1.4"
  },
  "dependencies": {
    "@signpdf/placeholder-plain": "^3.3.0",
    "electron-store": "^8.1.0",
    "form-data": "^4.0.1",
    "node-fetch": "^2.7.0",
    "node-forge": "^1.3.3",
    "node-qpdf2": "^6.0.0",
    "node-signpdf": "^3.0.0",
    "pdf-lib": "^1.17.1"
  },
  "'version'": "1.4.15"
}
