{
  "name": "tecnoescala",
  "product_name": "Tecnoescala",
  "version": "1.5.5",
  "description": "Aplicacion tecnoescala",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "watch": "nodemon --exec electron .",
    "build-win": "electron-builder --win"
  },
  "build": {
    "appId": "tecnoescala",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.ico"
    },
    "nsis": {
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "uninstallDisplayName": "Tecnoescala",
      "license": "LICENSE.md"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron-builder": "^22.13.1",
    "nodemon": "^3.1.4"
  },
  "dependencies": {
    "electron": "^33.0.2",
    "electron-store": "^8.1.0",
    "filereader": "^0.10.3",
    "form-data": "^4.0.1",
    "node-fetch": "^3.3.2"
  },
  "'version'": "1.4.15"
}
